{
  "flow_type": "data_pipe",
  "flow_subtype": "example_pipe",
  "flow_name": "example_data_pipe",
  "status": "released",
  "rows": [
    {
      "operation": "filter",
      "args_list": "value>1;",
      "input_source": "example_data_pipe_list"
    },
    {
      "operation": "append_columns",
      "args_list": "Text: Hello {@row.id};\nLabel: Another column;\nField: example_{@row.id}_field",
      "comments": "By default each operation will use output of previous unless defined"
    },
    {
      "operation": "filter_any",
      "args_list": "value%2===0;\nvalue===5;",
      "output_target": "intermediate_list | local_only;",
      "comments": "Output can be marked as local_only to prevent save"
    },
    {
      "operation": "filter",
      "args_list": "completed",
      "input_source": "intermediate_list",
      "output_target": "example_pipe_completed",
      "comments": "The same input can be used to produce multiple outputs"
    },
    {
      "operation": "filter",
      "args_list": "!completed",
      "input_source": "intermediate_list",
      "output_target": "example_pipe_uncompleted",
      "comments": "The outputs are not subject to the same 32 character name limits as standard data lists"
    }
  ]
}